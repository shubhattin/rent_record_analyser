{
  "name": "rent-record",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "dev:prod": "cross-env DB_MODE=PROD vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "migration:generate": "drizzle-kit generate:pg",
    "migration:push": "cd ./src/db && tsx migrate.ts",
    "migration:drop": "drizzle-kit drop",
    "migration:direct-push": "drizzle-kit push:pg",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "^2.10.2",
    "@skeletonlabs/tw-plugin": "^0.4.0",
    "@sveltejs/kit": "^2.5.26",
    "@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
    "@tailwindcss/forms": "^0.5.9",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "drizzle-kit": "^0.20.18",
    "jose": "^5.8.0",
    "mode-watcher": "^0.3.1",
    "postcss": "^8.4.45",
    "postcss-load-config": "^5.1.0",
    "postgres": "^3.4.4",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.6",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "svelte": "5.0.0-next.244",
    "svelte-check": "^4.0.1",
    "tailwindcss": "^3.4.10",
    "tslib": "^2.7.0",
    "tsx": "^4.19.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vite-node": "^2.0.5",
    "vite-plugin-tailwind-purgecss": "^0.3.3"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.6.10",
    "@fontsource/roboto": "^5.0.15",
    "@neondatabase/serverless": "^0.8.1",
    "@sveltejs/adapter-vercel": "^5.4.3",
    "@tanstack/svelte-query": "^5.55.2",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "clsx": "^2.1.1",
    "devalue": "^5.0.0",
    "drizzle-orm": "^0.29.5",
    "drizzle-zod": "^0.5.1",
    "lucide-svelte": "^0.395.0",
    "sass": "^1.78.0",
    "superjson": "^2.2.1",
    "svelte-icons-pack": "^3.1.3",
    "tailwind-merge": "^2.5.2",
    "trpc-svelte-query": "^2.1.0",
    "trpc-sveltekit": "^3.6.2",
    "zod": "^3.23.8"
  }
}
