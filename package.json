    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "@neondatabase/serverless": "^0.8.1",
    "@picocss/pico": "^1.5.11",
