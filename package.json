{
  "name": "rent-record",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:prod": "cross-env DB_MODE=PROD vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "migration:generate": "drizzle-kit generate:pg",
    "migration:push": "cd ./src/db && tsx migrate.ts",
    "migration:drop": "drizzle-kit drop",
    "migration:direct-push": "drizzle-kit push:pg",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@skeletonlabs/skeleton": "^2.10.1",
    "@skeletonlabs/tw-plugin": "^0.4.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "drizzle-kit": "^0.20.14",
    "postcss": "^8.4.32",
    "postcss-load-config": "^5.0.2",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.9",
    "svelte": "^4.2.7",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.3.6",
    "tslib": "^2.4.1",
    "tsx": "^4.7.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "vite-plugin-tailwind-purgecss": "^0.3.3"
  },
  "type": "module",
  "dependencies": {
    "@floating-ui/dom": "^1.6.5",
    "@neondatabase/serverless": "^0.8.1",
    "@sveltejs/adapter-vercel": "^5.1.0",
    "@trpc/client": "^10.45.1",
    "@trpc/server": "^10.45.1",
    "@types/jsonwebtoken": "^9.0.6",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.4",
    "drizzle-orm": "^0.29.3",
    "drizzle-zod": "^0.5.1",
    "jsonwebtoken": "^9.0.2",
    "lucide-svelte": "^0.395.0",
    "mode-watcher": "^0.3.0",
    "postgres": "^3.4.3",
    "sass": "^1.70.0",
    "svelte-icons-pack": "^3.1.3",
    "tailwind-merge": "^2.3.0",
    "trpc-sveltekit": "^3.5.26",
    "zod": "^3.22.4"
  }
}
