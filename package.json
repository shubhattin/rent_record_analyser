{
  "name": "rent-record",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev --host",
    "dev:prod": "cross-env DB_MODE=PROD vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "migration:generate": "drizzle-kit generate:pg",
    "migration:push": "cd ./src/db && tsx migrate.ts",
    "migration:drop": "drizzle-kit drop",
    "migration:direct-push": "drizzle-kit push:pg",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/kit": "^2.20.4",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/forms": "^0.5.10",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "drizzle-kit": "^0.20.18",
    "jose": "^5.10.0",
    "mode-watcher": "0.3.1",
    "postgres": "^3.4.5",
    "prettier": "^3.5.3",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "sass": "^1.86.3",
    "svelte": "^5.25.6",
    "svelte-check": "^4.1.5",
    "tailwindcss": "^4.1.3",
    "tslib": "^2.8.1",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3",
    "vite": "^6.2.5",
    "vite-node": "^2.1.9"
  },
  "type": "module",
  "dependencies": {
    "@builder.io/partytown": "^0.10.3",
    "@fontsource/roboto": "^5.2.5",
    "@neondatabase/serverless": "^1.0.0",
    "@skeletonlabs/skeleton": "^3.1.2",
    "@skeletonlabs/skeleton-svelte": "^1.2.0",
    "@sveltejs/adapter-vercel": "^5.7.0",
    "@tailwindcss/vite": "^4.1.3",
    "@tanstack/svelte-query": "^5.71.10",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "clsx": "^2.1.1",
    "devalue": "^5.1.1",
    "drizzle-orm": "^0.29.5",
    "drizzle-zod": "^0.5.1",
    "lucide-svelte": "^0.395.0",
    "superjson": "^2.2.2",
    "svelte-icons-pack": "^3.1.3",
    "tailwind-merge": "^2.6.0",
    "trpc-svelte-query": "^2.1.0",
    "trpc-sveltekit": "^3.6.3",
    "zod": "^3.24.2"
  }
}
